{
  "name": "curated-corpus-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:prisma-deploy && node dist/main.js",
    "dev": "npm run migrate:prisma-deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test:watch": "npm test -- --watchAll",
    "test-setup": "export $(egrep -v '^#' .docker/local.env | xargs -0) && ./.circleci/scripts/setup.sh --hosts",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "test-integrations:watch": "npm run test-integrations -- --watchAll",
    "lint": "eslint --fix-dry-run \"src/**/*.ts\"",
    "db:generate-prisma-client": "prisma generate",
    "db:prisma-seed": "npx prisma migrate reset --force && ts-node --emit=false prisma/seed.ts",
    "migrate:prisma-dev": "prisma migrate dev",
    "migrate:prisma-deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node --emit=false prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server-plugin-response-cache": "4.1.3",
    "@aws-sdk/client-eventbridge": "3.342.0",
    "@aws-sdk/client-s3": "^3.341.0",
    "@aws-sdk/lib-storage": "^3.342.0",
    "@devoxa/prisma-relay-cursor-connection": "3.1.0",
    "@pocket-tools/apollo-utils": "3.3.4",
    "@pocket-tools/ts-logger": "^1.3.0",
    "@prisma/client": "^4.16.2",
    "@sentry/node": "7.49.0",
    "@sentry/tracing": "7.49.0",
    "@snowplow/node-tracker": "3.5.0",
    "@types/luxon": "3.3.0",
    "cors": "2.8.5",
    "dataloader": "2.2.2",
    "date-fns": "2.29.3",
    "express": "4.18.2",
    "graphql-scalars": "1.22.2",
    "graphql-upload": "15.0.2",
    "luxon": "3.3.0",
    "metadata-scraper": "^0.2.61"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.341.0",
    "@faker-js/faker": "8.0.2",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@types/chai": "4.3.5",
    "@types/faker": "6.6.8",
    "@types/graphql-upload": "8.0.12",
    "@types/jest": "29.5.3",
    "@types/node": "18.16.19",
    "@types/sinon": "10.0.15",
    "chai": "4.3.7",
    "dotenv": "16.3.1",
    "form-data": "4.0.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "nock": "13.3.2",
    "nodemon": "3.0.1",
    "prisma": "^4.16.1",
    "sinon": "^17.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1"
  },
  "files": [
    "dist",
    "schema-admin.graphql",
    "schema-public.graphql",
    "schema-shared.graphql",
    "package.json"
  ]
}
