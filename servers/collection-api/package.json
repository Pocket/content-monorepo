{
  "name": "collection-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run db:generate-prisma-client && rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run db:generate-prisma-client && npm run migrate:prisma-deploy && node dist/main.js",
    "dev": "npm run migrate:prisma-deploy && npm run build && npm run watch",
    "test": "jest \"\\.spec\\.ts\"",
    "test-ci": "npm test",
    "test:watch": "npm test -- --watchAll",
    "test-setup": "export $(egrep -v '^#' .docker/local.env | xargs -0) && ./.circleci/scripts/setup.sh --hosts",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "test-integrations:watch": "npm run test-integrations -- --watchAll",
    "lint": "eslint --fix-dry-run \"src/**/*.ts\"",
    "db:generate-prisma-client": "prisma generate",
    "db:prisma-seed": "npx prisma migrate reset --force && ts-node --emit=false prisma/seed.ts",
    "migrate:prisma-dev": "prisma migrate dev",
    "migrate:prisma-deploy": "prisma migrate deploy",
    "migrate:prisma-reset": "prisma migrate reset --skip-seed --force"
  },
  "prisma": {
    "seed": "ts-node --emit=false prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server-plugin-response-cache": "4.1.3",
    "@aws-sdk/client-eventbridge": "3.529.1",
    "@aws-sdk/client-s3": "3.529.1",
    "@aws-sdk/lib-storage": "3.529.1",
    "@pocket-tools/apollo-utils": "3.4.2",
    "@pocket-tools/ts-logger": "^1.3.0",
    "@prisma/client": "^5.0.0",
    "@sentry/node": "7.110.1",
    "@sentry/tracing": "7.110.1",
    "cors": "2.8.5",
    "content-common": "workspace:*",
    "dataloader": "^2.2.2",
    "express": "4.19.2",
    "graphql-tag": "2.12.6",
    "graphql-upload": "15.0.2",
    "slugify": "1.6.6",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "@faker-js/faker": "8.4.1",
    "@types/chai": "4.3.5",
    "@types/jest": "29.5.12",
    "@types/graphql-upload": "8.0.12",
    "@types/node": "^20.12.7",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "chai": "4.3.7",
    "dotenv": "16.3.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "nock": "13.5.1",
    "nodemon": "3.0.1",
    "prisma": "^5.0.0",
    "sinon": "^15.0.1",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2"
  }
}
