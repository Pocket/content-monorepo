{
  "$schema": "https://unpkg.com/syncpack@12.3.0/dist/schema.json",
  "dependencyTypes": [
    "dev",
    "peer",
    "prod",
    "nodeEngine"
  ],
  "customTypes": {
    "nodeEngine": {
      "path": "engines.node",
      "strategy": "version"
    }
  },
  "versionGroups": [
    {
      "dependencies": [
        "@aws-sdk/**"
      ],
      "dependencyTypes": [
        "!dev"
      ],
      "pinVersion": "3.529.1",
      "label": "AWS SDK Dependencies should all have the same version"
    },
    {
      "dependencies": [
        "@sentry/**"
      ],
      "pinVersion": "7.112.2",
      "label": "AWS SDK Dependencies should all have the same version"
    },
    {
      "dependencies": [
        "*prisma/**",
        "prisma"
      ],
      "pinVersion": "5.20.0",
      "label": "Prisma"
    },
    {
      "dependencies": [
        "@types/**"
      ],
      "dependencyTypes": [
        "!dev"
      ],
      "isBanned": true,
      "label": "@types packages should only be under devDependencies",
      "packages": [
        "!tsconfig"
      ]
    }
  ]
}
