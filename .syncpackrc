{
  "versionGroups": [
    {
      "label": "Ensure AWS SDK dependencies always use the same version",
      "packages": ["**"],
      "dependencies": ["@aws-sdk/**"],
      "pinVersion": "3.529.1"
    },
    {
      "label": "Use workspace protocol for local packages, such as content-common",
      "dependencies": [$LOCAL],
      "pinVersion": "workspace:*"
    }
  ]
}
