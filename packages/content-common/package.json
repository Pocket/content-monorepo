{
  "name": "content-common",
  "version": "1.0.1",
  "description": "common code shared between multiple content services",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./snowplow": "./dist/snowplow/index.js",
    "./snowplow/test-helpers": "./dist/snowplow/test-helpers.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint": "eslint --fix-dry-run \"*.ts\"",
    "format": "eslint --fix \"**/*.ts\""
  },
  "author": "pocket",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@snowplow/node-tracker": "3.5.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "eslint-config-custom": "workspace:*",
    "jest": "29.7.0",
    "msw": "2.4.7",
    "ts-jest": "29.1.2",
    "tsconfig": "workspace:*"
  }
}
