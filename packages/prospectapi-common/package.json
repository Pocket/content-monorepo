{
  "name": "prospectapi-common",
  "version": "1.0.0",
  "description": "common code shared between api and lambda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "pnpm run build --watch",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint": "eslint --fix-dry-run \"**/*.ts\" \"test/**/*.ts\"",
    "format": "eslint --fix \"**/*.ts\" \"test/**/*.ts\""
  },
  "files": [
    "dist",
    "package.json"
  ],
  "author": "pocket",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.9.5",
    "@aws-sdk/client-dynamodb": "3.529.1",
    "@aws-sdk/lib-dynamodb": "3.529.1",
    "@aws-sdk/util-dynamodb": "3.529.1",
    "content-common": "workspace:*",
    "@sentry/node": "8.37.1",
    "cross-fetch": "3.1.5",
    "fetch-retry": "5.0.6",
    "graphql-tag": "2.12.6",
    "tldts": "5.7.87",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/jest": "29.5.12",
    "eslint-config-custom": "workspace:*",
    "jest": "29.7.0",
    "msw": "2.4.7",
    "ts-jest": "29.1.2",
    "tsup": "8.2.4",
    "tsconfig": "workspace:*",
    "typescript": "5.6.3",
    "@types/node": "^22"
  }
}
